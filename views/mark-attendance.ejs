<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Mark Attendance - Face Attendance</title>
    <style>
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        body {
            font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            min-height: 100vh;
            padding: 20px;
        }

        .container {
            max-width: 700px;
            margin: 20px auto;
            background: white;
            border-radius: 20px;
            box-shadow: 0 20px 60px rgba(0, 0, 0, 0.3);
            padding: 30px;
        }

        h1 {
            text-align: center;
            color: #667eea;
            margin-bottom: 30px;
        }

        .header-bar {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: 20px;
            flex-wrap: wrap;
            gap: 10px;
        }

        .back-btn {
            display: inline-block;
            padding: 10px 20px;
            background: #f0f0f0;
            color: #333;
            text-decoration: none;
            border-radius: 8px;
            font-size: 0.9em;
        }

        .back-btn:hover {
            background: #e0e0e0;
        }

        .user-info {
            display: flex;
            align-items: center;
            gap: 15px;
        }

        .user-badge {
            display: inline-flex;
            align-items: center;
            gap: 8px;
            padding: 8px 15px;
            background: linear-gradient(135deg, #667eea, #764ba2);
            color: white;
            border-radius: 20px;
            font-size: 0.9em;
            font-weight: 600;
        }

        .logout-btn {
            padding: 8px 15px;
            background: #ef4444;
            color: white;
            text-decoration: none;
            border-radius: 8px;
            font-size: 0.9em;
            transition: background 0.3s;
        }

        .logout-btn:hover {
            background: #dc2626;
        }

        .split-container {
            display: flex;
            flex-direction: column;
            min-height: 70vh;
        }

        .camera-section {
            flex: 1;
            background: #000;
            border-radius: 15px;
            overflow: hidden;
            position: relative;
            min-height: 50vh;
            display: flex;
            align-items: center;
            justify-content: center;
        }

        #video {
            width: 100%;
            height: 100%;
            object-fit: cover;
            display: block;
        }

        #canvas {
            display: none;
        }

        #capturedImage {
            width: 100%;
            height: 100%;
            object-fit: cover;
            display: none;
        }

        .camera-placeholder {
            color: white;
            text-align: center;
            padding: 40px;
        }

        .camera-placeholder i {
            font-size: 4em;
            margin-bottom: 20px;
            opacity: 0.5;
        }

        .controls-section {
            padding: 20px 0;
        }

        .controls {
            display: flex;
            flex-direction: column;
            gap: 15px;
            margin: 20px 0;
        }

        button {
            width: 100%;
            padding: 18px;
            font-size: 1.1em;
            border: none;
            border-radius: 12px;
            cursor: pointer;
            font-weight: 600;
            transition: all 0.3s;
            display: flex;
            align-items: center;
            justify-content: center;
            gap: 10px;
        }

        button i {
            font-size: 1.2em;
        }

        .btn-primary {
            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            color: white;
        }

        .btn-primary:hover:not(:disabled) {
            transform: translateY(-2px);
            box-shadow: 0 5px 15px rgba(102, 126, 234, 0.4);
        }

        .btn-success {
            background: #10b981;
            color: white;
        }

        .btn-success:hover:not(:disabled) {
            background: #059669;
            transform: translateY(-2px);
            box-shadow: 0 5px 15px rgba(16, 185, 129, 0.4);
        }

        .btn-capture {
            background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
            color: white;
            font-size: 1.2em;
            padding: 20px;
        }

        .btn-capture:hover:not(:disabled) {
            transform: translateY(-2px);
            box-shadow: 0 5px 15px rgba(102, 126, 234, 0.4);
        }

        button:disabled {
            opacity: 0.6;
            cursor: not-allowed;
        }

        .message {
            padding: 15px;
            border-radius: 10px;
            margin: 20px 0;
            text-align: center;
            font-weight: 600;
        }

        .message.success {
            background: #d4edda;
            color: #155724;
            border: 2px solid #c3e6cb;
        }

        .message.error {
            background: #f8d7da;
            color: #721c24;
            border: 2px solid #f5c6cb;
        }

        .message.info {
            background: #d1ecf1;
            color: #0c5460;
            border: 2px solid #bee5eb;
        }

        .message.warning {
            background: #fff3cd;
            color: #856404;
            border: 2px solid #ffeaa7;
        }

        .troubleshooting {
            background: #fff3cd;
            border: 2px solid #ffc107;
            border-radius: 10px;
            padding: 20px;
            margin: 20px 0;
            display: none;
        }

        .troubleshooting.show {
            display: block;
        }

        .troubleshooting h3 {
            color: #856404;
            margin-bottom: 15px;
        }

        .troubleshooting ol {
            margin-left: 20px;
            line-height: 1.8;
        }

        .troubleshooting li {
            margin-bottom: 10px;
        }

        .troubleshooting strong {
            color: #856404;
        }

        .troubleshooting code {
            background: #fff;
            padding: 2px 6px;
            border-radius: 3px;
            font-family: monospace;
        }

        .status {
            text-align: center;
            padding: 10px;
            margin: 10px 0;
            color: #666;
            font-style: italic;
        }

        .result-info {
            background: #f0fdf4;
            border: 2px solid #86efac;
            padding: 20px;
            border-radius: 12px;
            margin: 20px 0;
            display: none;
        }

        .result-info.show {
            display: block;
        }

        .result-info.error {
            background: #fef2f2;
            border-color: #fca5a5;
        }

        .result-info h3 {
            color: #059669;
            margin-bottom: 15px;
            display: flex;
            align-items: center;
            gap: 10px;
        }

        .result-info.error h3 {
            color: #dc2626;
        }

        .result-info p {
            margin: 8px 0;
            font-size: 1.05em;
        }

        .result-info strong {
            color: #1f2937;
        }

        .user-info strong {
            color: #333;
        }

        .loading {
            text-align: center;
            margin: 20px 0;
        }

        .spinner {
            border: 3px solid #f3f3f3;
            border-top: 3px solid #667eea;
            border-radius: 50%;
            width: 40px;
            height: 40px;
            animation: spin 1s linear infinite;
            margin: 0 auto;
        }

        @keyframes spin {
            0% { transform: rotate(0deg); }
            100% { transform: rotate(360deg); }
        }

        @media (max-width: 600px) {
            .container {
                padding: 15px;
            }

            .header-bar {
                flex-direction: column;
                gap: 10px;
            }

            h1 {
                font-size: 1.5em;
            }

            .split-container {
                min-height: 60vh;
            }

            .camera-section {
                min-height: 40vh;
            }

            button {
                font-size: 1em;
                padding: 15px;
            }

            .btn-capture {
                font-size: 1.1em;
                padding: 18px;
            }
        }
    </style>
</head>
<body>
    <div class="container">
        <div class="header-bar">
            <a href="/" class="back-btn">‚Üê Back to Home</a>
            <div class="user-info">
                <div class="user-badge">
                    üë§ <%= user.username %> (<%= user.role %>)
                </div>
                <a href="/logout" class="logout-btn">Logout</a>
            </div>
        </div>
        
        <h1>Mark Attendance</h1>

        <div class="split-container">
            <div class="camera-section" id="cameraSection">
                <div class="camera-placeholder" id="cameraPlaceholder">
                    <i class="fas fa-camera"></i>
                    <p>Click "Allow Camera Access" to begin</p>
                </div>
                <video id="video" autoplay playsinline style="display: none;"></video>
                <canvas id="canvas"></canvas>
                <img id="capturedImage" alt="Captured face">
            </div>
        </div>

        <div id="permissionRequest" class="message info" style="display: block;">
            <h3 style="margin-bottom: 10px;">üì∑ Camera Access Required</h3>
            <p style="margin-bottom: 15px;">This app needs camera access to scan your face for attendance.</p>
            <button id="requestCameraBtn" class="btn-primary" style="width: auto; padding: 12px 30px;">Allow Camera Access</button>
            <button id="showTroubleshootingBtn" class="btn-secondary" style="width: auto; padding: 12px 30px; margin-left: 10px;">Having Issues?</button>
        </div>

        <div id="troubleshooting" class="troubleshooting">
            <h3>üõ†Ô∏è Camera Access Troubleshooting</h3>
            
            <p><strong>Current URL:</strong> <code id="currentUrl"></code></p>
            <p style="margin-bottom: 15px;">Camera should work on <code>localhost</code> or <code>HTTPS</code> sites.</p>
            
            <h4 style="margin-top: 15px; margin-bottom: 10px;">Quick Fixes:</h4>
            <ol>
                <li><strong>Check URL:</strong> Make sure you're using <code>http://localhost:3000</code> (not 127.0.0.1 or IP address)</li>
                <li><strong>Close Other Apps:</strong> Close any apps using your camera (Zoom, Teams, Skype, etc.)</li>
                <li><strong>Refresh Page:</strong> Press <code>Ctrl+F5</code> to hard refresh</li>
                <li><strong>Check Browser Permissions:</strong></li>
            </ol>
            
            <h4 style="margin-top: 15px; margin-bottom: 10px;">Browser-Specific Instructions:</h4>
            
            <p><strong>Chrome/Edge:</strong></p>
            <ul style="margin-left: 20px; margin-bottom: 10px;">
                <li>Click the üîí or üé• icon in the address bar</li>
                <li>Set Camera to "Allow"</li>
                <li>Refresh the page</li>
            </ul>
            
            <p><strong>Firefox:</strong></p>
            <ul style="margin-left: 20px; margin-bottom: 10px;">
                <li>Click the üõà icon in the address bar</li>
                <li>Go to Permissions ‚Üí Camera ‚Üí Allow</li>
                <li>Refresh the page</li>
            </ul>
            
            <p><strong>Safari (Mac):</strong></p>
            <ul style="margin-left: 20px; margin-bottom: 10px;">
                <li>Safari ‚Üí Settings ‚Üí Websites ‚Üí Camera</li>
                <li>Allow access for localhost</li>
                <li>Refresh the page</li>
            </ul>
            
            <p><strong>Mobile Browsers:</strong></p>
            <ul style="margin-left: 20px; margin-bottom: 15px;">
                <li>Settings ‚Üí Safari/Chrome ‚Üí Camera ‚Üí Allow</li>
                <li>Make sure site has camera permission</li>
            </ul>
            
            <button id="retryAfterTroubleshooting" class="btn-primary" style="margin-top: 10px;">Try Again</button>
        </div>

        <div id="status" class="status">Click "Allow Camera Access" to begin</div>

        <div id="resultInfo" class="result-info">
            <h3><i class="fas fa-check-circle"></i> <span id="resultTitle">Face Recognized!</span></h3>
            <p><strong>Name:</strong> <span id="userName"></span></p>
            <p><strong>Employee ID:</strong> <span id="userEmployeeId"></span></p>
            <p><strong>Confidence:</strong> <span id="confidence"></span></p>
        </div>

        <div class="controls" id="controls" style="display: none;">
            <button id="captureBtn" class="btn-capture">
                <i class="fas fa-camera"></i> Capture Face
            </button>
            <button id="confirmBtn" class="btn-success" style="display: none;">
                <i class="fas fa-check"></i> Confirm & Mark Attendance
            </button>
            <button id="retakeBtn" class="btn-primary" style="display: none;">
                <i class="fas fa-redo"></i> Retake Photo
            </button>
        </div>

        <div id="message"></div>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/@vladmandic/face-api@1.7.12/dist/face-api.js"></script>
    <script>
        const video = document.getElementById('video');
        const canvas = document.getElementById('canvas');
        const ctx = canvas.getContext('2d');
        const capturedImage = document.getElementById('capturedImage');
        const cameraSection = document.getElementById('cameraSection');
        const cameraPlaceholder = document.getElementById('cameraPlaceholder');
        const captureBtn = document.getElementById('captureBtn');
        const confirmBtn = document.getElementById('confirmBtn');
        const retakeBtn = document.getElementById('retakeBtn');
        const status = document.getElementById('status');
        const message = document.getElementById('message');
        const resultInfo = document.getElementById('resultInfo');
        const resultTitle = document.getElementById('resultTitle');
        const userName = document.getElementById('userName');
        const userEmployeeId = document.getElementById('userEmployeeId');
        const confidenceEl = document.getElementById('confidence');
        const requestCameraBtn = document.getElementById('requestCameraBtn');
        const permissionRequest = document.getElementById('permissionRequest');
        const controls = document.getElementById('controls');
        const showTroubleshootingBtn = document.getElementById('showTroubleshootingBtn');
        const troubleshooting = document.getElementById('troubleshooting');
        const retryAfterTroubleshooting = document.getElementById('retryAfterTroubleshooting');
        const currentUrl = document.getElementById('currentUrl');

        let modelsLoaded = false;
        let cameraReady = false;
        let registeredUsers = [];
        let labeledDescriptors = [];
        let faceMatcher = null;
        let matchedUser = null;

        // Load face-api models
        async function loadModels() {
            try {
                status.textContent = 'Loading face detection models...';
                const MODEL_URL = 'https://cdn.jsdelivr.net/npm/@vladmandic/face-api@1.7.12/model';
                
                await Promise.all([
                    faceapi.nets.ssdMobilenetv1.loadFromUri(MODEL_URL),
                    faceapi.nets.faceLandmark68Net.loadFromUri(MODEL_URL),
                    faceapi.nets.faceRecognitionNet.loadFromUri(MODEL_URL)
                ]);
                
                modelsLoaded = true;
                console.log('All models loaded successfully');
                status.textContent = 'Models loaded! Loading registered users...';
                await loadRegisteredUsers();
            } catch (error) {
                console.error('Error loading models:', error);
                status.textContent = 'Error loading models. Please refresh the page.';
                showMessage('Failed to load face detection models', 'error');
            }
        }

        // Load registered users from database
        async function loadRegisteredUsers() {
            try {
                const response = await fetch('/api/users-descriptors');
                const data = await response.json();
                
                if (response.ok && data.users.length > 0) {
                    registeredUsers = data.users;
                    
                    // Create labeled face descriptors
                    labeledDescriptors = data.users.map(user => {
                        const descriptors = user.descriptor.map(desc => new Float32Array(desc));
                        return new faceapi.LabeledFaceDescriptors(user.id.toString(), descriptors);
                    });
                    
                    // Create face matcher with more lenient threshold (0.6 = 60% similarity required)
                    // Higher value = more lenient matching
                    faceMatcher = new faceapi.FaceMatcher(labeledDescriptors, 0.6);
                    
                    if (cameraReady) {
                        status.textContent = `‚úì Ready! ${data.users.length} user(s) registered. Click "Capture Face" to begin.`;
                        captureBtn.disabled = false;
                    }
                } else {
                    status.textContent = 'No registered users found.';
                    showMessage('No registered users found. Please register a user first.', 'error');
                }
            } catch (error) {
                console.error('Error loading users:', error);
                status.textContent = 'Error loading users.';
                showMessage('Failed to load registered users', 'error');
            }
        }

        // Request and start camera
        async function startCamera() {
            try {
                status.textContent = 'Requesting camera access...';
                const stream = await navigator.mediaDevices.getUserMedia({
                    video: { 
                        facingMode: 'user',
                        width: { ideal: 1280 },
                        height: { ideal: 720 }
                    }
                });
                video.srcObject = stream;
                cameraReady = true;
                
                // Hide permission request, show camera
                permissionRequest.style.display = 'none';
                cameraPlaceholder.style.display = 'none';
                video.style.display = 'block';
                controls.style.display = 'flex';
                
                status.textContent = 'Camera ready! Loading face detection models...';
                showMessage('‚úì Camera access granted! Loading AI models...', 'success');
                
                // Load models after camera is ready
                loadModels();
            } catch (error) {
                console.error('Error accessing camera:', error);
                cameraReady = false;
                
                let errorMsg = '‚ùå Camera Access Failed. ';
                let suggestions = '<br><br><strong>Possible solutions:</strong><ul style=\"margin-left: 20px; text-align: left;\">';
                
                if (error.name === 'NotAllowedError') {
                    errorMsg += 'Camera permission was denied.';
                    suggestions += '<li>Click the üîí or üé• icon in the address bar and allow camera</li>';
                    suggestions += '<li>Or click \"Having Issues?\" button below for detailed instructions</li>';
                } else if (error.name === 'NotFoundError') {
                    errorMsg += 'No camera found on this device.';
                    suggestions += '<li>Make sure your device has a camera</li>';
                } else if (error.name === 'NotReadableError') {
                    errorMsg += 'Camera is already in use by another application.';
                    suggestions += '<li>Close Zoom, Teams, Skype, or other video apps</li>';
                } else if (error.name === 'TypeError') {
                    errorMsg += 'Secure connection required.';
                    suggestions += '<li>Use <code>http://localhost:3000</code></li>';
                }
                
                suggestions += '</ul>';
                
                status.textContent = 'Camera access failed';
                showMessage(errorMsg + suggestions, 'error');
                
                if (error.name === 'NotAllowedError' || error.name === 'TypeError') {
                    setTimeout(() => {
                        troubleshooting.classList.add('show');
                    }, 2000);
                }
            }
        }

        // Capture image from video
        captureBtn.addEventListener('click', async () => {
            // Validate prerequisites
            if (!modelsLoaded) {
                showMessage('Models are still loading. Please wait...', 'warning');
                return;
            }
            if (!faceMatcher) {
                showMessage('Face matcher not initialized. Please refresh the page.', 'error');
                return;
            }
            if (!cameraReady) {
                showMessage('Camera not ready. Please allow camera access.', 'error');
                return;
            }
            if (video.readyState !== video.HAVE_ENOUGH_DATA) {
                showMessage('Video not ready. Please wait a moment and try again.', 'warning');
                return;
            }

            captureBtn.disabled = true;
            status.textContent = 'Capturing and analyzing face...';
            resultInfo.classList.remove('show');

            try {
                // Set canvas size and capture frame
                canvas.width = video.videoWidth;
                canvas.height = video.videoHeight;
                
                if (canvas.width === 0 || canvas.height === 0) {
                    throw new Error('Invalid canvas dimensions');
                }
                
                ctx.drawImage(video, 0, 0);
                
                // Show captured image with high quality
                const imageData = canvas.toDataURL('image/jpeg', 0.95);
                capturedImage.src = imageData;
                capturedImage.style.display = 'block';
                video.style.display = 'none';
                
                status.textContent = 'Analyzing face...';
                
                // Detect face with landmarks and descriptor
                const detection = await faceapi
                    .detectSingleFace(canvas, new faceapi.SsdMobilenetv1Options({ 
                        minConfidence: 0.3,
                        maxResults: 1
                    }))
                    .withFaceLandmarks()
                    .withFaceDescriptor();

                if (detection) {
                    const result = faceMatcher.findBestMatch(detection.descriptor);
                    
                    console.log('Match result:', result.label, 'Distance:', result.distance);
                    
                    // Check if match is found and within acceptable range
                    if (result.label !== 'unknown' && result.distance < 0.6) {
                        const userId = result.label;
                        matchedUser = registeredUsers.find(u => u.id === userId);
                        
                        if (matchedUser) {
                            const confidence = ((1 - result.distance) * 100).toFixed(1);
                            
                            userName.textContent = matchedUser.name;
                            userEmployeeId.textContent = matchedUser.employeeId;
                            confidenceEl.textContent = `${confidence}%`;
                            
                            resultInfo.classList.add('show');
                            resultInfo.classList.remove('error');
                            resultTitle.textContent = 'Face Recognized!';
                            
                            confirmBtn.style.display = 'block';
                            retakeBtn.style.display = 'block';
                            captureBtn.style.display = 'none';
                            
                            status.textContent = `‚úì Face matched: ${matchedUser.name} (${confidence}% confidence)`;
                            showMessage(`‚úì Face recognized: ${matchedUser.name}`, 'success');
                        }
                    } else {
                        console.log('No match found. Best distance:', result.distance);
                        const matchScore = ((1 - result.distance) * 100).toFixed(0);
                        showError(`Face not recognized. Please ensure you are registered. (Match score: ${matchScore}%)`);
                        retakeBtn.style.display = 'block';
                        captureBtn.style.display = 'none';
                    }
                } else {
                    console.log('No face detected in image');
                    showError('No face detected. Please ensure your face is clearly visible and well-lit.');
                    retakeBtn.style.display = 'block';
                    captureBtn.style.display = 'none';
                }
            } catch (error) {
                console.error('Error detecting face:', error);
                console.error('Error details:', error.message, error.stack);
                
                // Reset to video view on error
                capturedImage.style.display = 'none';
                video.style.display = 'block';
                captureBtn.style.display = 'block';
                captureBtn.disabled = false;
                
                showError(`Error analyzing face: ${error.message}. Please try again.`);
                showMessage('Error analyzing face. Please ensure good lighting and try again.', 'error');
            }
        });

        // Retake photo
        retakeBtn.addEventListener('click', () => {
            capturedImage.style.display = 'none';
            video.style.display = 'block';
            captureBtn.style.display = 'block';
            captureBtn.disabled = false;
            confirmBtn.style.display = 'none';
            retakeBtn.style.display = 'none';
            resultInfo.classList.remove('show');
            matchedUser = null;
            status.textContent = 'Ready to capture. Position your face in camera.';
        });

        // Confirm attendance
        confirmBtn.addEventListener('click', async () => {
            if (!matchedUser) return;

            confirmBtn.disabled = true;
            status.textContent = 'Marking attendance...';

            try {
                const response = await fetch('/api/confirm-attendance', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify({
                        userId: matchedUser.id
                    })
                });

                const data = await response.json();

                if (response.ok) {
                    showMessage(`‚úì Attendance marked successfully for ${data.user.name} at ${data.time}`, 'success');
                    status.textContent = 'Attendance marked successfully!';
                    
                    setTimeout(() => {
                        resultInfo.classList.remove('show');
                        capturedImage.style.display = 'none';
                        video.style.display = 'block';
                        captureBtn.style.display = 'block';
                        captureBtn.disabled = false;
                        confirmBtn.style.display = 'none';
                        confirmBtn.disabled = false;
                        retakeBtn.style.display = 'none';
                        matchedUser = null;
                        status.textContent = 'Ready for next person.';
                    }, 3000);
                } else {
                    showMessage(data.error || 'Failed to mark attendance', 'error');
                    confirmBtn.disabled = false;
                    status.textContent = 'Failed to mark attendance. Try again.';
                }
            } catch (error) {
                console.error('Error marking attendance:', error);
                showMessage('Error marking attendance. Please try again.', 'error');
                confirmBtn.disabled = false;
                status.textContent = 'Error marking attendance.';
            }
        });

        function showError(message) {
            resultInfo.classList.add('show', 'error');
            resultTitle.textContent = 'Detection Failed';
            userName.textContent = message;
            userEmployeeId.textContent = '';
            confidenceEl.textContent = '';
            status.textContent = message;
            showMessage(message, 'error');
        }

        function showMessage(text, type) {
            message.innerHTML = `<div class=\"message ${type}\">${text}</div>`;
            setTimeout(() => {
                message.innerHTML = '';
            }, 5000);
        }

        // Request camera button
        requestCameraBtn.addEventListener('click', async () => {
            requestCameraBtn.disabled = true;
            requestCameraBtn.textContent = 'Requesting...';
            troubleshooting.classList.remove('show');
            await startCamera();
            if (!cameraReady) {
                requestCameraBtn.disabled = false;
                requestCameraBtn.textContent = 'Retry Camera Access';
            }
        });

        // Show troubleshooting
        showTroubleshootingBtn.addEventListener('click', () => {
            troubleshooting.classList.toggle('show');
            showTroubleshootingBtn.textContent = troubleshooting.classList.contains('show') 
                ? 'Hide Instructions' 
                : 'Having Issues?';
        });

        // Retry after troubleshooting
        retryAfterTroubleshooting.addEventListener('click', () => {
            requestCameraBtn.click();
        });

        // Initialize
        async function initializeApp() {
            currentUrl.textContent = window.location.href;
            
            const isLocalhost = window.location.hostname === 'localhost' || 
                               window.location.hostname === '127.0.0.1';
            const isHttps = window.location.protocol === 'https:';
            
            if (!isLocalhost && !isHttps) {
                showMessage('‚ö†Ô∏è Warning: Camera may not work without HTTPS. Use http://localhost:3000', 'warning');
            }
        }

        initializeApp();
    </script>
</body>
</html>
